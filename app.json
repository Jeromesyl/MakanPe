{
  "expo": {
    "name": "MakanPe",
    "slug": "MakanPe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/makanpeicon.png",
    "splash": {
      "image": "./assets/splashback.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-image-picker"
    ],
    "description": "An Android app to help make choosing food easier.",
    "githubUrl": "https://github.com/TeamJerNi/MakanPe.git"
  }
}
